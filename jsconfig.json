{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2017",
    "baseUrl": "./src/",
    "paths": {
      "@/*": ["./src/*"],
      "components/*": ["./src/components/*"],
      "features/*": ["./src/features/*"],
      "lib/*": ["./src/lib/*"],
      "reducers/*": ["./src/reducers/*"],
      "styles/*": ["./src/styles/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
